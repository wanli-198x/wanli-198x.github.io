{
  "name": "vitepress-plugin-permalink",
  "type": "module",
  "version": "1.2.1",
  "description": "扫描 Markdown 文档，生成永久链接",
  "author": {
    "name": "teeker",
    "email": "2456019588@qq.com",
    "url": "https://vp.teek.top"
  },
  "keywords": [
    "vitepress",
    "vite",
    "plugin",
    "permalink"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Kele-Bingtang/vitepress-theme-teek/tree/master/plugins/vitepress-plugin-permalink",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kele-Bingtang/vitepress-theme-teek.git"
  },
  "bugs": {
    "url": "https://github.com/Kele-Bingtang/vitepress-theme-teek/issues"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./usePermalink": {
      "types": "./dist/usePermalink.d.ts",
      "import": "./dist/usePermalink.mjs",
      "require": "./dist/usePermalink.cjs"
    },
    "./rewrites": {
      "types": "./dist/rewrites.d.ts",
      "import": "./dist/rewrites.mjs",
      "require": "./dist/rewrites.cjs"
    },
    "./dist/components/*.vue": "./dist/components/*.vue"
  },
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "fs-extra": "^11.2.0",
    "vite": "^6.0.7",
    "vitepress": "^1.6.3"
  },
  "peerDependencies": {
    "vitepress": "^1.6.3"
  },
  "scripts": {
    "stub": "unbuild --stub",
    "build": "unbuild"
  }
}