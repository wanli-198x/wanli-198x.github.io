#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/elon/Downloads/my-blog/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.41.0_@types+node@24.9.1_postcss@8.5.6_sass@1.9_e9504eec3a8f3854a6fef515db041827/node_modules/vitepress/bin/node_modules:/Users/elon/Downloads/my-blog/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.41.0_@types+node@24.9.1_postcss@8.5.6_sass@1.9_e9504eec3a8f3854a6fef515db041827/node_modules/vitepress/node_modules:/Users/elon/Downloads/my-blog/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.41.0_@types+node@24.9.1_postcss@8.5.6_sass@1.9_e9504eec3a8f3854a6fef515db041827/node_modules:/Users/elon/Downloads/my-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/elon/Downloads/my-blog/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.41.0_@types+node@24.9.1_postcss@8.5.6_sass@1.9_e9504eec3a8f3854a6fef515db041827/node_modules/vitepress/bin/node_modules:/Users/elon/Downloads/my-blog/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.41.0_@types+node@24.9.1_postcss@8.5.6_sass@1.9_e9504eec3a8f3854a6fef515db041827/node_modules/vitepress/node_modules:/Users/elon/Downloads/my-blog/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.41.0_@types+node@24.9.1_postcss@8.5.6_sass@1.9_e9504eec3a8f3854a6fef515db041827/node_modules:/Users/elon/Downloads/my-blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vitepress@1.6.4_@algolia+client-search@5.41.0_@types+node@24.9.1_postcss@8.5.6_sass@1.9_e9504eec3a8f3854a6fef515db041827/node_modules/vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../../../../vitepress@1.6.4_@algolia+client-search@5.41.0_@types+node@24.9.1_postcss@8.5.6_sass@1.9_e9504eec3a8f3854a6fef515db041827/node_modules/vitepress/bin/vitepress.js" "$@"
fi
